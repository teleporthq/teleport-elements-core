{
  "name": "teleport-elements-core-html",
  "version": "0.0.3",
  "type": "mapping",
  "library": "teleport-elements-core",
  "target": "html",
  "maps": {
    "Text": {
      "type": "span"
    },
    "Image": {
      "type": "img",
      "props": {
        "src": "props.uri"
      }
    },
    "View": {
      "type": "div"
    },
    "H1": {
      "type": "h1"
    },
    "H2": {
      "type": "h1"
    },
    "H3": {
      "type": "h1"
    },
    "H4": {
      "type": "h1"
    },
    "H5": {
      "type": "h1"
    },
    "Link": {
      "type": "a"
    },
    "Button": {
      "type": "button"
    },
    "TextInput": {
      "type": "input",
      "props": {
        "type": "text"
      }
    },
    "TextAreaInput": {
      "type": "textarea"
    },
    "Svg": {
      "type": "svg"
    }
  }
}
